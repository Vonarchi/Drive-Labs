{
  "name": "drive-labs-orchestrator",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:css": "tailwindcss -i ./styles/globals.css -o ./public/styles.css",
    "start": "next start",
    "start:preview": "next start",
    "lint": "eslint .",
    "test": "vitest run",
    "test:ui": "vitest",
    "test:e2e": "playwright test",
    "test:lh": "lhci autorun",
    "test:ping": "curl -s http://localhost:3000/health || true",
    "test:run": "curl -s -X POST http://localhost:3000/pipeline/run -H 'content-type: application/json' -d '{\"projectSpec\":{\"name\":\"labs-demo\"}}' || true",
    "db:push": "supabase db push",
    "seed": "node scripts/seed.mjs",
    "seed:full": "node scripts/seed-comprehensive.mjs",
    "seed:test-user": "tsx scripts/seed-test-user.ts",
    "seed:users": "tsx scripts/seed-users.ts",
    "user:create": "node scripts/create-test-user.js",
    "user:list": "node scripts/user-management.js list",
    "user:delete": "node scripts/user-management.js delete",
    "foreman": "tsx scripts/foreman.ts",
    "team:gen": "pnpm foreman",
    "team:ci": "pnpm build && pnpm test && pnpm test:e2e && pnpm test:lh",
    "deploy": "pnpm build && supabase db push && supabase functions deploy --no-verify-jwt",
    "pnpm:dev": "pnpm dev",
    "pnpm:build": "pnpm build",
    "pnpm:start": "pnpm start",
    "pnpm:test:ping": "pnpm run test:ping",
    "pnpm:test:run": "pnpm run test:run",
    "verify:vercel": "node scripts/verify-vercel.js"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.20.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.76.0",
    "archiver": "^6.0.2",
    "change-case": "^5.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "eta": "^3.5.0",
    "execa": "^9.6.0",
    "express": "^4.19.2",
    "lucide-react": "^0.546.0",
    "next": "14.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^6.6.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "tslib": "^2.6.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.0.0",
    "@types/archiver": "^6.0.3",
    "@types/express": "^4.17.23",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vercel/style-guide": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.38.0",
    "jsdom": "^24.0.0",
    "lhci": "^4.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.0"
  }
}




